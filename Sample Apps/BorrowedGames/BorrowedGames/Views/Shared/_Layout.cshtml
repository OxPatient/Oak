<!DOCTYPE html>
<html>
<head>
    <title>Borrowed Games - @ViewBag.Title</title>
    <link href="@Url.Content("~/Content/screen.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.tmpl.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-position.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modit/modit.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/underscore.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/backbone.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/app/growl.js")" type="text/javascript"></script>
</head>

<body>
    <hgroup id="banner">
      <header>
        <div style="font-size: 80px">Borrowed Games</div>
        <div style="font-size: 30px">
          Save Money. Game On.
        <div>
      </header>
    </hgroup>
    <script>
        $(function () {
            growl.init();
        });
    </script>
    <div id="main">
      @RenderBody()
    </div>
    <div style="margin-left: auto; margin-right: auto; padding: 10px; margin-bottom: 5px; color: black;">
      <div style="float: right; font-size: small;">
          Borrowed Games | Copyright 2009 | Scratchwork Development LLC
      </div>
    </div>
  </body>
</html>
